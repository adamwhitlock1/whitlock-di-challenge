{
  "name": "php-contact-form",
  "version": "1.0.0",
  "description": "Welcome to the Dealer Inspire PHP Code challenge.",
  "main": "index.js",
  "scripts": {
    "servePHP": "php -S 127.0.0.1:9999 -t public",
    "compile": "webpack --mode development --watch --hide-modules",
    "cypress": "cypress open",
    "serveE2E": "WAIT_FOR_MESSAGE='Reloading Browsers...' cypressAppWatcher npm run compile",
    "build": "webpack --mode production",
    "regen": "composer dump-autoload -o",
    "killserver": "lsof -ti:9999 | xargs kill",
    "watchTestsE2E": "npm-run-all --parallel servePHP serveE2E cypress",
    "watchTestsPHP": "phpunit-watcher watch",
    "watchALL": "npm-run-all --parallel watchTestsE2E watchTestsPHP",
    "dev": "npm-run-all --parallel servePHP compile"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/dealerinspire/php-contact-form#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-stage-3": "^7.0.0",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.0.0",
    "cypress": "^3.4.1",
    "cypress-app-watcher-preprocessor": "^1.1.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "lodash": "^4.17.11",
    "parsleyjs": "^2.9.1"
  }
}
